        <Param val="" valType="extendedCode" updates="constant" name="Begin Experiment"/>
        <Param val="" valType="extendedCode" updates="constant" name="Begin JS Experiment"/>
        <Param val="matlab_send(&quot;fb&quot;);&amp;#10;console.log(key_resp.keys, &quot;was pressed\n&quot;);&amp;#10;try {&amp;#10;    if ((key_resp.keys === null)) {&amp;#10;        fb_text = &quot;please respond within a shorter time period.\nPress any key to continue.&quot;;&amp;#10;        timing_map[&quot;dFb&quot;] = TIMEOUT_DURATION;&amp;#10;        fb_col = &quot;white&quot;;&amp;#10;        stim_map[&quot;loop_iter&quot;] -= 1;&amp;#10;        stim_map[&quot;cross_en&quot;] = false;&amp;#10;    } else {&amp;#10;        if (key_resp.corr) {&amp;#10;            fb_text = &quot;Correct!&quot;;&amp;#10;            fb_col = &quot;green&quot;;&amp;#10;        } else {&amp;#10;            fb_text = &quot;Incorrect&quot;;&amp;#10;            fb_col = &quot;red&quot;;&amp;#10;        }&amp;#10;    }&amp;#10;} catch(e) {&amp;#10;    console.log(&quot;ERROR: no key_resp keyboard component written.&quot;);&amp;#10;}&amp;#10;stim_map[&quot;loop_iter&quot;] += 1;&amp;#10;" valType="extendedCode" updates="constant" name="Begin JS Routine"/>
        <Param val="# send signal to SIMULINK&amp;#10;matlab_send(&quot;fb&quot;)&amp;#10;&amp;#10;# display feedback to log&amp;#10;print(key_resp.keys,&quot;was pressed\n&quot;)&amp;#10;try:&amp;#10;    if key_resp.keys == None:&amp;#10;        # add timeout text, decrement loop, enable fixation&amp;#10;        fb_text = 'please respond within a shorter time period.\nPress any key to continue.'&amp;#10;        timing_map[&quot;dFb&quot;] = TIMEOUT_DURATION&amp;#10;        fb_col = 'white'&amp;#10;        stim_map[&quot;loop_iter&quot;] -= 1&amp;#10;        stim_map[&quot;cross_en&quot;] = False&amp;#10;    elif key_resp.corr:&amp;#10;        fb_text = 'Correct!'&amp;#10;        fb_col = 'green'&amp;#10;    else:&amp;#10;        fb_text = 'Incorrect'&amp;#10;        fb_col = 'red'&amp;#10;        &amp;#10;except:&amp;#10;    print('ERROR: no key_resp keyboard component written.')&amp;#10;&amp;#10;&amp;#10;# increment loop, enable cross if loop reaches max&amp;#10;stim_map[&quot;loop_iter&quot;] += 1&amp;#10;" valType="extendedCode" updates="constant" name="Begin Routine"/>
        <Param val="Auto-&gt;JS" valType="str" updates="None" name="Code Type"/>
        <Param val="" valType="extendedCode" updates="constant" name="Each Frame"/>
        <Param val="" valType="extendedCode" updates="constant" name="Each JS Frame"/>
        <Param val="" valType="extendedCode" updates="constant" name="End Experiment"/>
        <Param val="" valType="extendedCode" updates="constant" name="End JS Experiment"/>
        <Param val="console.log(&quot;loop_iter =&quot;, stim_map[&quot;loop_iter&quot;], &quot;\n&quot;, &quot;loop_count =&quot;, stim_map[&quot;loop_count&quot;], &quot;\n&quot;);&amp;#10;timing_map[&quot;dFb&quot;] = DEFAULT_FEEDBACK;&amp;#10;if ((stim_map[&quot;loop_iter&quot;] === stim_map[&quot;loop_count&quot;])) {&amp;#10;    stim_map[&quot;loop_iter&quot;] = 0;&amp;#10;    stim_map[&quot;char_length&quot;] += CHARACTER_INCREMENT;&amp;#10;    stim_map[&quot;cross_en&quot;] = false;&amp;#10;}&amp;#10;console.log(&quot;timing information is: \n&quot;);&amp;#10;console.log(&quot;timing_map[\&quot;dCross\&quot;]:&quot;, timing_map[&quot;dCross&quot;]);&amp;#10;console.log(&quot;timing_map[\&quot;dBlank\&quot;]:&quot;, timing_map[&quot;dBlank&quot;]);&amp;#10;console.log(&quot;timing_map[\&quot;dPrompt\&quot;]:&quot;, timing_map[&quot;dPrompt&quot;]);&amp;#10;console.log(&quot;timing_map[\&quot;dResponse\&quot;]:&quot;, timing_map[&quot;dResponse&quot;]);&amp;#10;console.log(&quot;timing_map[\&quot;dFb\&quot;]:&quot;, timing_map[&quot;dFb&quot;]);&amp;#10;console.log(&quot;current loop that ended is:&quot;, currentLoop.thisTrial);&amp;#10;psychoJS.experiment.addData(&quot;reactionTime&quot;, key_resp.rt);&amp;#10;psychoJS.experiment.addData(&quot;accuracy&quot;, ((key_resp.corr === 1) ? &quot;correct&quot; : &quot;incorrect&quot;));&amp;#10;psychoJS.experiment.addData(&quot;prompt&quot;, stim_map[&quot;string_prompt&quot;]);&amp;#10;psychoJS.experiment.addData(&quot;character&quot;, stim_map[&quot;key_prompt&quot;]);&amp;#10;if ((key_resp.keys === null)) {&amp;#10;    currentLoop.rewindTrials();&amp;#10;} else {&amp;#10;    RT_list.push(key_resp.rt);&amp;#10;    ACC_list.push(key_resp.corr);&amp;#10;}&amp;#10;" valType="extendedCode" updates="constant" name="End JS Routine"/>
        <Param val="# display loop information &amp;#10;print(&quot;loop_iter =&quot;, stim_map[&quot;loop_iter&quot;], '\n',&amp;#10;      &quot;loop_count =&quot;, stim_map[&quot;loop_count&quot;], '\n')&amp;#10;&amp;#10;# ensure that fb is reset to 1&amp;#10;timing_map[&quot;dFb&quot;] = DEFAULT_FEEDBACK&amp;#10;# increment characters after specified loop count&amp;#10;if stim_map[&quot;loop_iter&quot;] == stim_map[&quot;loop_count&quot;]:&amp;#10;    stim_map[&quot;loop_iter&quot;] = 0&amp;#10;    stim_map[&quot;char_length&quot;] += CHARACTER_INCREMENT&amp;#10;    stim_map[&quot;cross_en&quot;] = False&amp;#10;&amp;#10;# print out debugging information&amp;#10;print(&quot;timing information is: \n&quot;)&amp;#10;print(&quot;timing_map[\&quot;dCross\&quot;]:&quot;, timing_map[&quot;dCross&quot;])&amp;#10;print(&quot;timing_map[\&quot;dBlank\&quot;]:&quot;, timing_map[&quot;dBlank&quot;])&amp;#10;print(&quot;timing_map[\&quot;dPrompt\&quot;]:&quot;, timing_map[&quot;dPrompt&quot;])&amp;#10;print(&quot;timing_map[\&quot;dResponse\&quot;]:&quot;, timing_map[&quot;dResponse&quot;])&amp;#10;print(&quot;timing_map[\&quot;dFb\&quot;]:&quot;, timing_map[&quot;dFb&quot;])&amp;#10;print(&quot;current loop that ended is:&quot;, currentLoop.thisTrial)&amp;#10;&amp;#10;# log reaction time and accuracy percentage&amp;#10;thisExp.addData('reactionTime', key_resp.rt)&amp;#10;thisExp.addData('accuracy', &quot;correct&quot; if (key_resp.corr == 1) else &quot;incorrect&quot;)&amp;#10;thisExp.addData('prompt', stim_map[&quot;string_prompt&quot;])&amp;#10;thisExp.addData('character', stim_map[&quot;key_prompt&quot;])&amp;#10;# rewind trial to overwrite timeout data&amp;#10;if key_resp.keys == None:&amp;#10;    currentLoop.rewindTrials()&amp;#10;else:&amp;#10;    RT_list.append(key_resp.rt)&amp;#10;    ACC_list.append(key_resp.corr)&amp;#10;    " valType="extendedCode" updates="constant" name="End Routine"/>
        <Param val="False" valType="bool" updates="None" name="disabled"/>
        <Param val="code_fb" valType="code" updates="None" name="name"/>
