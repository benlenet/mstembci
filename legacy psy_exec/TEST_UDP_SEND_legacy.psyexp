        <Param val="console.log(p_key_resp.keys, &quot;was pressed\n&quot;);&amp;#10;p_timing_shift_text = &quot;&quot;;&amp;#10;try {&amp;#10;    if ((p_key_resp.keys === null)) {&amp;#10;        fb_text = &quot;please respond within a shorter time period.\nPress any key to continue.&quot;;&amp;#10;        fb_col = &quot;white&quot;;&amp;#10;        timing_map[&quot;p_dFb&quot;] = TIMEOUT_DURATION;&amp;#10;        stim_map[&quot;loop_iter&quot;] -= 1;&amp;#10;        stim_map[&quot;cross_en&quot;] = false;&amp;#10;    } else {&amp;#10;        if (p_key_resp.corr) {&amp;#10;            fb_text = &quot;Correct!&quot;;&amp;#10;            fb_col = &quot;green&quot;;&amp;#10;        } else {&amp;#10;            fb_text = &quot;Incorrect&quot;;&amp;#10;            fb_col = &quot;red&quot;;&amp;#10;        }&amp;#10;    }&amp;#10;} catch(e) {&amp;#10;    console.log(&quot;Make sure that you have:\n1. a routine with a keyboard component in it called \&quot;key_resp\&quot;\n 2. In the key_Resp component in the \&quot;data\&quot; tab select \&quot;Store Correct\&quot;.\n in the \&quot;Correct answer\&quot; field use \&quot;$corrAns\&quot; (where corrAns is a column header in your conditions file indicating the correct key press&quot;);&amp;#10;}&amp;#10;stim_map[&quot;loop_iter&quot;] += 1;&amp;#10;if (((stim_map[&quot;char_length&quot;] === 3) &amp;&amp; (stim_map[&quot;loop_iter&quot;] === 2))) {&amp;#10;    p_timing_shift_text = &quot;the stimulus will now speed up&quot;;&amp;#10;}&amp;#10;" valType="extendedCode" updates="constant" name="Begin JS Routine"/>
        <Param val="&amp;#10;&amp;#10;# display feedback to log&amp;#10;print(p_key_resp.keys,&quot;was pressed\n&quot;)&amp;#10;&amp;#10;&amp;#10;p_timing_shift_text = &quot;&quot;&amp;#10;&amp;#10;try:&amp;#10;    if p_key_resp.keys == None:&amp;#10;        # add timeout text, decrement loop, enable fixation&amp;#10;        fb_text = 'please respond within a shorter time period.\nPress any key to continue.'&amp;#10;        fb_col = 'white'&amp;#10;        # add duration for user to pause/orient&amp;#10;        timing_map[&quot;p_dFb&quot;] = TIMEOUT_DURATION&amp;#10;        # adjust loop values&amp;#10;        stim_map[&quot;loop_iter&quot;] -= 1&amp;#10;        # reenable cross&amp;#10;        stim_map[&quot;cross_en&quot;] = False&amp;#10;    elif p_key_resp.corr:&amp;#10;        fb_text = 'Correct!'&amp;#10;        fb_col = 'green'&amp;#10;    else:&amp;#10;        fb_text = 'Incorrect'&amp;#10;        fb_col = 'red'        &amp;#10;except:&amp;#10;    print('Make sure that you have:\n1. a routine with a keyboard component in it called &quot;key_resp&quot;\n 2. In the key_Resp component in the &quot;data&quot; tab select &quot;Store Correct&quot;.\n in the &quot;Correct answer&quot; field use &quot;$corrAns&quot; (where corrAns is a column header in your conditions file indicating the correct key press')&amp;#10;&amp;#10;&amp;#10;# increment loop, enable cross if loop reaches max&amp;#10;stim_map[&quot;loop_iter&quot;] += 1&amp;#10;if ((stim_map[&quot;char_length&quot;] == 3) and (stim_map[&quot;loop_iter&quot;] == 2)):&amp;#10;    p_timing_shift_text = &quot;the stimulus will now speed up&quot;&amp;#10;" valType="extendedCode" updates="constant" name="Begin Routine"/>
